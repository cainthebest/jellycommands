{
  "name": "jellycommands",
  "version": "1.0.0-next.39",
  "author": "GHOST",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./ambient": {
      "types": "./ambient.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "tsup-node --watch src",
    "build": "tsup-node",
    "test": "tsm node_modules/uvu/bin.js tests \".*\\.test\\.ts\"",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier --check --plugin-search-dir=. src",
    "lint:eslint": "eslint src",
    "fix": "prettier --write --plugin-search-dir=. src"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "discord.js": "^14.7.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "tsm": "^2.3.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "uvu": "^0.5.6"
  },
  "peerDependencies": {
    "discord.js": "^14.0.0"
  },
  "dependencies": {
    "axios": "^1.3.3",
    "discord-api-types": "^0.37.35",
    "joi": "^17.8.3",
    "totalist": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghostdevv/jellycommands.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/ghostdevv/jellycommands/issues"
  },
  "homepage": "https://github.com/ghostdevv/jellycommands#readme"
}